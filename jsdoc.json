{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["src/"],
    "includePattern": ".(js|ts)$",
    "excludePattern": "(node_modules/|docs)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "docdash" : {
      "static" : false,
      "sort" : true
    }
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "readme": "README.md"
  }
}
